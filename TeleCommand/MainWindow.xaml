<Window x:Class="TeleCommand.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeleCommand"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="lbl_Bot_ID" Content="Bot ID" HorizontalAlignment="Left" Margin="30,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tb_Bot_ID" Height="23" TextWrapping="Wrap" Text=""  VerticalAlignment="Top" Margin="78,13,10,0" AcceptsReturn="False"/>
        <Label x:Name="lbl_Username" Content="Username" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tb_Username" Height="23" Margin="78,45,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" AcceptsReturn="False" />
        <RichTextBox x:Name="tb_Output" Margin="10,73,10,30" AcceptsReturn="True" IsReadOnly="True" VerticalScrollBarVisibility="Visible"/>
        <Button x:Name="btn_stop" Content="Stop" HorizontalAlignment="Right" Margin="0,0,10,5" VerticalAlignment="Bottom" Width="75" Click="btn_stop_Click"/>
        <Button x:Name="btn_start" Content="Start" HorizontalAlignment="Right" Margin="0,0,90,5" VerticalAlignment="Bottom" Width="75" Click="btn_start_Click"/>


    </Grid>
</Window>
